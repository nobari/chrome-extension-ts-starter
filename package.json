{
  "name": "ts-chrome-extension-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for Chrome Extensions written in TypeScript and bundled with Webpack",
  "main": "index.ts",
  "scripts": {
    "watch": "webpack --config webpack.config.ts -w",
    "clean": "rm -rf extension",
    "start": "webpack serve --config webpack.config.ts --open popup",
    "build": "webpack --config webpack.config.ts",
    "build:prod": "webpack --env production --config webpack.config.ts"
  },
  "keywords": [
    "webpack",
    "TypeScript",
    "Chrome Extension"
  ],
  "author": "Sadegh Nobari",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.258",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {}
}
